{
  "name": "email-workflow-backend",
  "version": "1.0.0",
  "description": "Backend for Custom Email Workflow Tool",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register -r module-alias/register dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register -r module-alias/register src/index.ts",
    "build": "tsc && tsc-alias",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prestart": "npm run build"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.10.0",
    "googleapis": "^128.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.0.3",
    "openai": "^4.28.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8"
  },
  "_moduleAliases": {
    "@": "./dist",
    "@config": "./dist/config",
    "@controllers": "./dist/controllers",
    "@models": "./dist/models",
    "@routes": "./dist/routes",
    "@services": "./dist/services",
    "@utils": "./dist/utils"
  }
}
